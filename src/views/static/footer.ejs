<script>
  tippy(".tippy", { 
    arrow: true, animation: "scale", delay: 3
  });
  async function getCount(id) {
    let x = await axios.get('/api/activeSound?id='+id)
            .then(res => res.data);
    return x.count;
  }
</script>

